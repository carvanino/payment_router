{
    "name": "akinola/payment-router",
    "description": "A Laravel package for smart payment gateway routing",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Akinola\\PaymentRouter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Akinola",
            "email": "akinolatofunmi.tech@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Akinola\\PaymentRouter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akinola\\PaymentRouter\\PaymentRouterServiceProvider"
            ],
            "aliases": {
                "PaymentRouter": "Akinola\\PaymentRouter\\Facades\\PaymentRouter"
            }
        }
    },
    "minimum-stability": "dev"
}
